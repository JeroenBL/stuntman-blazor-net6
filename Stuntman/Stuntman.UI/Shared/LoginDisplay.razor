@inject NavigationManager _navigationManager

<AuthorizeView>
    <Authorized>
        <MudIconButton Icon="@Icons.Material.Filled.Logout" Color="Color.Primary" aria-label="Logout" OnClick='@(() => _navigationManager.NavigateTo("/Identity/Account/LogOut", true))'></MudIconButton>
    </Authorized>
    <NotAuthorized>
        <MudIconButton Icon="@Icons.Material.Filled.Login" Color="Color.Primary" aria-label="Login" OnClick='@(() => _navigationManager.NavigateTo("/Identity/Account/Login", true))'></MudIconButton>
    </NotAuthorized>
</AuthorizeView>
